{
    "root": true,
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "script"
    },
    "extends": ["eslint:recommended", "prettier", "plugin:react/recommended"],
    "env": {
        "es2021": true,
        "browser": true
    },
    "rules": {
        "no-console": "error",
        "no-await-in-loop": "error"
    }
}
